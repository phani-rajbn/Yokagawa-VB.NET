Public Class MathClass
    Public Function AddFunc(firstVal As Double, secondval As Double) As Double
        Return firstVal + secondval
    End Function
    Public Function SubFunc(firstVal As Double, secondval As Double) As Double
        Return firstVal - secondval
    End Function
    Public Function MulFunc(firstVal As Double, secondval As Double) As Double
        Return firstVal * secondval
    End Function
    Public Function DivFunc(firstVal As Double, secondval As Double) As Double
        Return firstVal / secondval
    End Function
End Class

''' <summary>
''' Represents an Employee of the System
''' </summary>
Public Class Employee
    ''' <summary>
    ''' Gets or Sets the ID of the Employee
    ''' </summary>
    Public Property EmpID As Integer
    ''' <summary>
    ''' Gets or Sets the Name of the Employee
    ''' </summary>
    Public Property EmpName As String
    ''' <summary>
    ''' Gets or Sets the Address or location of the Employee
    ''' </summary>
    Public Property EmpAddress As String
    ''' <summary>
    ''' Gets or Sets the Salary for the Employee
    ''' </summary>
    Public Property EmpSalary As Integer
End Class
